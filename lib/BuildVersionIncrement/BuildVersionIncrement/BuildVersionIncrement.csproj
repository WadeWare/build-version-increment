    <PostBuildEvent>
      @echo off

      IF EXIST "%25USERPROFILE%25\My Documents" SET DOCUMENTSFOLDER=%25USERPROFILE%25\My Documents
      IF EXIST "%25USERPROFILE%25\Documents" SET DOCUMENTSFOLDER=%25USERPROFILE%25\Documents

      if "$(ConfigurationName)" == "Debug VS2005" goto DebugVS2005
      if "$(ConfigurationName)" == "Debug VS2008" goto DebugVS2008
      if "$(ConfigurationName)" == "Debug" goto DebugVS2010
      if "$(ConfigurationName)" == "Release" goto DebugVS2010
      if "$(ConfigurationName)" == "Debug CommandLine Vista" goto DebugVS2008
      if not "$(ConfigurationName)" == "Install" goto exit

      :CreateZips
      copy /y "$(TargetPath)" "$(ProjectDir)Install\"
      del "$(ProjectDir)Install\*.zip"
      cd "$(ProjectDir)Install\

      call CreateVSI.cmd

      echo Creating release zip ...
      zip "BuildVersionIncrement v2.5.zip" *.vsi

      goto exit

      :DebugVS2005
      SET ADDINSFOLDER=%25DOCUMENTSFOLDER%25\Visual Studio 2005\Addins\

      goto CreateAddinFile

      :DebugVS2008

      SET ADDINSFOLDER=%25DOCUMENTSFOLDER%25\Visual Studio 2008\Addins

      goto CreateAddinFile

      :DebugVS2010

      SET ADDINSFOLDER=%25DOCUMENTSFOLDER%25\Visual Studio 2010\Addins

      goto CreateAddinFile

      :CreateAddinFile
      echo Creating AddinFile in "%25ADDINSFOLDER%25"
      IF NOT EXIST "%25ADDINSFOLDER%25\old" MKDIR "%25ADDINSFOLDER%25\old"
      move /y "%25ADDINSFOLDER%25\BuildVersionIncrement*.Addin" "%25ADDINSFOLDER%25\old"

      "$(ProjectDir)Addin Files\BuildAddinFile.exe" /a:"$(TargetPath)" /f:"%25ADDINSFOLDER%25\BuildVersionIncrement.$(ConfigurationName).Addin"

      goto CreateZips

      :exit
</PostBuildEvent>
